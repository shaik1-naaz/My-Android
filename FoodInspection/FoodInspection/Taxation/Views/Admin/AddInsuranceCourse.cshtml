@model Core.CourseInsurance
@{
    ViewBag.Title = "Add Insurance Course";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/jquery-2.2.3.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/Jquery-Validations.js"></script>
<section class="aftrer_h">
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/Jquery-Validations.js"></script>
    <div class="container_m">
        <div class="add_new">
            <h3>
                <a href="@Url.Action("Insurance", "Admin")"><i class="fa fa-arrow-circle-left"></i></a> Add New Insurance Module
            </h3>
            <div class="add_new_body">
                @using (@Html.BeginForm("AddInsuranceCourse", "Admin", FormMethod.Post, new { id = "addInsurance" }))
                {
                    <div class="input_sec">
                        <label>Course Module</label>
                        @Html.TextBoxFor(m => m.CourseName, new { MaxLength="50" })
                    </div>
                    <div class="lgn_btns">
                        @Html.ActionLink("Cancel", "Insurance", "Admin", null, new { @class = "btn grey" })
                        <input type="submit" name="Add" value="Add" class="btn blue_btn" />
                    </div>
                }
            </div>
        </div>
    </div>
</section>