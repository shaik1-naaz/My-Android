<script type="text/javascript">
    $(document).ready(function () {

        debugger;

        @Html.Raw(TempData["List"]);

        debugger;
        $(".draggable").draggable({
            revert: true,
            helper: 'clone',
            start: function (event, ui) {
                $(this).fadeTo('fast', 0.5);
            },
            stop: function (event, ui) {
                $(this).fadeTo(0, 1);
            }
        });
        $(".droppable").droppable({
            hoverClass: 'active',
            drop: function (event, ui) {
               
                if ($(ui.draggable).attr("id") == $(this).attr("id")) {
                    $('.correct').addClass('efct');
                    $('.overlay').show();
                    setTimeout(function () {
                        $('.correct').removeClass('efct');
                        $('.overlay').hide();
                    }, 1000);
                    $(this).val($(ui.draggable).text().trim());
                    $(ui.draggable).draggable('disable').css('color', 'green');
                    $(this).droppable('disable');

                    var answer = JSON.stringify($(ui.draggable).text().trim());
                    var id = JSON.stringify($(this).attr("answerid"));
                    debugger;
                    $.ajax({
                        url: '@Url.Action("UpdateAnswer", "Test")',
                        type: 'POST',
                        data: '{ "Answer":' + answer + ', "Id": ' + id + ' }',
                        contentType: 'application/json; charset=utf-8',
                        success: function (result) {
                        },
                        error: function () {
                            alert("error");
                        }
                    });

                    return;
                }
                else {
                    $('.wrong').addClass('efct');
                    $('.overlay').show();
                    setTimeout(function () {
                        $('.wrong').removeClass('efct');
                        $('.overlay').hide();
                    }, 1000);
                    return;
                }
            }
        });
    });

</script>



